{
  "name": "czosnek",
  "version": "1.5.47",
  "description": "An html and css template mapping library for bindings",
  "main": "czosnek.js",
  "scripts": {
    "test": "node ./test/run.js",
    "debug": "node --inspect-brk ./test/run.js",
    "build": "node ./build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keleko34/czosnek.git"
  },
  "keywords": [
    "czosnek",
    "mapping",
    "binding"
  ],
  "author": "keleko34",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keleko34/czosnek/issues"
  },
  "homepage": "https://github.com/keleko34/czosnek#readme",
  "devDependencies": {
    "benchmark": "2.1.4",
    "chai": "4.2.0",
    "google-closure-compiler-js": "20200112.0.0",
    "lodash": "4.17.15",
    "mocha": "10.2.0",
    "platform": "1.3.5",
    "sinon": "8.0.4",
    "sinon-chai": "3.4.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    },
    "globals": {
      "define": true,
      "module": true,
      "czosnek": true,
      "Uint8Array": true
    },
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2017
    }
  }
}
